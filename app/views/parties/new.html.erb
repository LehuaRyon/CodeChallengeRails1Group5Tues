<h1>Party</h1>

<% if @party.errors.any? %>
  <div id="error_explanation">
    <h2><%= pluralize(@party.errors.count, "error") %> prohibited this article from being saved:</h2>

    <ul>
      <% @party.errors.each do |error| %>
        <li><%= error.full_message %></li>
      <% end %>
    </ul>
  </div>
<% end %>

<%= form_with model: @party do |f|%>
    <%= f.label :name%>
    <%= f.text_field :name%>

    <%= f.label :date%>
    <%= f.datetime_field :date%>
    <%= f.label :category%>
    <%= f.text_field :category%>
    <%= f.label :supplies%>
    <%= f.text_field :supplies%>
    <%= f.label :budget%>
    <%= f.number_field :budget%>

    <%=f.submit%>
    <%end%>